#include <stdio.h>

int main() {
    struct student 
    {
        int roll;
        char name[20];
        int age;
    };
    struct student std[10];
    int i;
    printf("enter 10 student records");
    for(i=0;i<10;i++)
{
 scanf("%d%s%d",&std[i].roll,&std[i].name,&std[i].age);   
}
for(i=0;i<10;i++)
{
    printf("%d%s%d",std[1].roll,std[1].name,std[1].age);
}
printf("\n");

    return 0;
}
