#include <stdio.h>
int main() {
int n;
printf("month no.");
scanf("%d",&n);
switch(n)
{
case 1:
printf("janurary");
break;
case 2:
printf("feb.");
break;
case 3:
printf("march");
break;
case 4:
printf("april");
break;
case 5:
printf("may");
break;
case 6:
printf("june");
break;
case 7:
printf("july");
break;
case 8:
printf("aug");
break;
case 9:
printf("sep");
break;
case 10:
printf("oct");
break;
case 11:
printf("nov");
break;
case 12:
printf("December");
break;
default:
printf("invalid");

}
    return 0;
}
